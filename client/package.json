{
  "name": "sequence-client",
  "version": "0.0.1",
  "description": "Sequence Client",
  "type": "module",
  "scripts": {
    "watch": "npm-watch",
    "reload": "reload -b -d build",
    "static": "mkdir -p build/ && rsync -a --delete --exclude js/ --exclude css/ static/ build/",
    "sass": "mkdir -p build/css && sass scss/styles.scss build/css/styles.css",
    "webpack": "webpack",
    "webpack-prod": "webpack --mode production",
    "build": "npm-run-all --parallel webpack sass static",
    "build-prod": "npm-run-all --parallel webpack-prod sass static",
    "test": "jest",
    "clean": "rm -rf build/*"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "../common/ts/test/test-setup.ts"
    ]
  },
  "watch": {
    "static": {
      "patterns": [
        "static/**"
      ],
      "extensions": "html,css,png,gif,mp3,ttf,svg"
    },
    "sass": {
      "patterns": [
        "scss/**"
      ],
      "extensions": "scss"
    },
    "webpack": {
      "patterns": [
        "ts/**"
      ],
      "extensions": "js,jsx,ts,tsx"
    }
  },
  "author": "Jezzamon",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/plugin-proposal-decorators": "^7.23.9",
    "@babel/plugin-transform-runtime": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-typescript": "^7.23.3",
    "@tsconfig/recommended": "^1.0.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node-localstorage": "^1.3.3",
    "@types/uuid": "^9.0.7",
    "babel-loader": "^9.1.3",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "node-localstorage": "^3.0.5",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.11.0",
    "reload": "^3.2.1",
    "sass": "^1.70.0",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.8",
    "lit": "^3.1.1",
    "socket.io-client": "^4.7.4",
    "uuid": "^9.0.1"
  }
}
